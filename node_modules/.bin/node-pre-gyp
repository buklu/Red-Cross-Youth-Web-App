D:/School/3rd_yr/1st Sem/CIS2205-DesignProject/Red-Cross-Youth-Web-App/#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,/,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node-pre-gyp/bin/node-pre-gyp/bin/node-pre-gyp" "$@"
  ret=$?
fi
exit $ret
